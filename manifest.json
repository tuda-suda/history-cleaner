{
  "manifest_version": 2,
  "name": "history-cleaner",
  "description": "Automatically remove pages that match given keywords from browser history",
  "version": "1.0",
  "icons": {
    "64": "icons/icon.png"
  },
  "background": {
    "scripts": ["background_script.js"]
  },
  "browser_action": {
    "default_icon": {
      "64": "icons/icon.png"
    },
    "default_popup": "popup/index.html",
    "default_title": "History cleaner"
  },
  "permissions": [
    "storage", 
    "history"
  ] 
}